<div class="modal-layout" id="ModalArchive">
    <div class="modal-scroll">
        <div class="modal-background" onclick="onTriggerModalArchive()"></div>
        <div class="archive-modal-layout">
            <div class="archive-modal">
                <div class="archive-modal-header-wrapper">
                    <div class="archive-modal-header">
                        <p class="archive-modal-header__title">This profile was moved to archive</p>
                        <img class="archive-modal-header__icon" src="/img/icons/closeIcon.svg" alt="close" onclick="onTriggerModalArchive()"/>
                    </div>
                </div>

                <div class="archive-modal-footer">
                    <button class="archive-modal-footer__buttonOk" onclick="onTriggerModalArchive()">Ok</button>
                </div>
            </div>
        </div>
    </div>
</div>